{
  "search.exclude": {
    "**/cache": true,
    "**/dist": true
  },
  "workbench.colorCustomizations": {
    "tab.activeBorderTop": "#f23668",
    "tab.activeBorder": "#0000"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.tabSize": 2,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
